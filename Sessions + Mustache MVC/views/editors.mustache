{{>header}}

{{>navigation}}

<div class="row">
  <div class="leftcolumn">
    <div class="card">
      <h2>Editors</h2>
      <table>
        <tr> <th> Username </th> <th> Password </th> <th> Level </th> <th> Delete </th> </tr>
        {{ #users }}
        <tr> <td> {{ username }} </td> <td> {{ password }} </td> <td> {{ level }} </td> <td> <a href="editors/userdelete/{{ username }}">D</a> </td> </tr>
        {{ /users }}
      </table>
    </div>
    <div class="card">
      <h2>Articles</h2>
      <table>
        <tr> <th> Title </th> <th> Author </th> <th> Delete </th> </tr>
        {{ #articles }}
        <tr> <td> {{ title }} </td> <td> {{ username }} </td> <td> <a href="editors/articledelete/{{ title }}">D</a> </td> </tr>
        {{ /articles }}
      </table>
    </div>
  </div>

  {{>sidebar}}

</div>

{{>footer}}
